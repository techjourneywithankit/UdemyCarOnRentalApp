        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Get_System_Threshold</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No, Not Entered</defaultConnectorLabel>
        <rules>
            <name>Yes_Coupon_code_Entered</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Enter_Coupon_Code</leftValueReference>
                <operator>IsBlank</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
