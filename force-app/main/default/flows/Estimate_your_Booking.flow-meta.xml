            </connector>
            <label>Yes , Coupon code Entered</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>frmBookingDays</name>
        <dataType>Number</dataType>
        <expression>IF({!Start_Date_Of_Booking} =={!End_Date_Of_Booking}, 1, {!End_Date_Of_Booking} - {!Start_Date_Of_Booking})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>frmBookingPrice</name>
        <dataType>Currency</dataType>
        <expression>{!BookingDays} * {!Get_Car_Details.Rental_Rate_Per_Day__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>frmNetPrice</name>
        <dataType>Currency</dataType>
        <expression>IF(NOT(ISBLANK({!Enter_Coupon_Code})),{!BookingPrice} - (({!Get_Coupon_Code.Discount_Percentage__c}/100) * {!BookingPrice}),{!BookingPrice})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>frmSecurityDeposit</name>
        <dataType>Currency</dataType>
        <expression>VALUE({!Get_System_Threshold.Value__c})/100 * {!NetPrice}</expression>
        <scale>2</scale>
    </formulas>
