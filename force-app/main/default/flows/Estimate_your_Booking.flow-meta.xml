    <decisions>
        <name>Check_Coupon_Code_Validity</name>
        <label>Check Coupon Code Validity</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Set_Coupon_Code_Not_Valid_Flag</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No, Not valid</defaultConnectorLabel>
        <rules>
            <name>Yes_it_is_valid</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Coupon_Code</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
